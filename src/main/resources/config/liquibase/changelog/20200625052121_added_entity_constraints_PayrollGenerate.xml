<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity PayrollGenerate.
    -->
    <changeSet id="20200625052121-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="employee_id"
                                 baseTableName="payroll_generate"
                                 constraintName="fk_payroll_generate_employee_id"
                                 referencedColumnNames="id"
                                 referencedTableName="employee"/>

        <addForeignKeyConstraint baseColumnNames="employee_salary_id"
                                 baseTableName="payroll_generate"
                                 constraintName="fk_payroll_generate_employee_salary_id"
                                 referencedColumnNames="id"
                                 referencedTableName="employee_salary"/>

    </changeSet>
</databaseChangeLog>
