<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity Employee.
    -->
    <changeSet id="20200620054903-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="employee_salary_id"
                                 baseTableName="employee"
                                 constraintName="fk_employee_employee_salary_id"
                                 referencedColumnNames="id"
                                 referencedTableName="employee_salary"/>

        <addForeignKeyConstraint baseColumnNames="employee_information_id"
                                 baseTableName="employee"
                                 constraintName="fk_employee_employee_information_id"
                                 referencedColumnNames="id"
                                 referencedTableName="employee_information"/>

    </changeSet>
</databaseChangeLog>
